<forms>
    <form name="wpforms">
        <settings>
            <property>form_title</property>
            <property>form_desc</property>
            <property>submit_text</property>
            <property>submit_text_processing</property>
            <property type="ARRAY" properties="message">confirmations</property>
            <property type="ARRAY" properties="subject,message">notifications</property>
            <property>conversational_forms_title</property>
            <property>conversational_forms_description</property>
            <property>form_pages_title</property>
            <property>form_pages_description</property>
            <property>form_pages_footer</property>
        </settings>
        <field>
            <property>label</property>
            <property>description</property>
            <property>placeholder</property>
            <property>simple_placeholder</property>
            <property>simple_default</property>
            <property>first_placeholder</property>
            <property>first_default</property>
            <property>middle_placeholder</property>
            <property>middle_default</property>
            <property>last_placeholder</property>
            <property>last_default</property>
            <property>confirmation_placeholder</property>
            <property>default_value</property>
            <property type="ARRAY" properties="label">choices</property>
            <property type="ARRAY">rows</property>
            <property type="ARRAY">columns</property>
            <property>date_placeholder</property>
            <property>time_placeholder</property>
            <property>title</property>
            <property>prev</property>
            <property>next</property>
        </field>
    </form>
</forms>